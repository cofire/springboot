<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cf.dao.mapper.custom.SystemCustomMapper">
	<!-- 获取用户权限 -->
	<select id="getSysparamsList" parameterType="java.lang.String" resultType="java.util.HashMap">
		SELECT *
		  FROM sys_params
		  <where> 
			1=1 
		</where> 
	</select>
</mapper>